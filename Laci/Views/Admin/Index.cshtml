@using Microsoft.AspNetCore.Authentication
@{
    ViewData["Title"] = "LACI - Admin";
}

<nav aria-label="breadcrumb" class="mt-3">
    <ol class="breadcrumb">
        <li class="breadcrumb-item active" aria-current="page">Import Data</li>
    </ol>
</nav>

<form enctype="multipart/form-data" method="post">
    <div class="form-row">
        <div class="col-12 col-sm-4 col-md-3 my-3">
            <input type="date" class="form-control" name="date" required>
        </div>
        <div class="col-12 col-sm-6 col-md-7 my-auto">
            <input type="file" name="file" required>
        </div>
        <div class="col-12 col-sm-2 col-md-2 my-3">
            <button type="submit" class="btn btn-primary float-sm-right">Upload</button>
        </div>
    </div>
</form>



<h2>Claims</h2>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h2>Properties</h2>

<dl>
    @foreach (var prop in (await Context.AuthenticateAsync()).Properties.Items)
    {
        <dt>@prop.Key</dt>
        <dd>@prop.Value</dd>
    }
</dl>